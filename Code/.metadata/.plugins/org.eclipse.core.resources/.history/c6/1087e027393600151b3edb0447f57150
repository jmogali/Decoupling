
public class ConnectionNotEstablished extends Exception{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	ConnectionNotEstablished(String stAgent_agent)
	{
		String parts[] = stAgent_agent.split("_");
		m_Receiver = parts[0];
		m_Sender = parts[1];
	}

	public String toString()
	{
		return "No Connection between : " + m_Sender + " to: "+ m_Receiver;
	}

}
