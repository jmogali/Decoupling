package Network_Related;

public class Inter_agent_constraint_encap extends STN_Edge{

	private final String m_str_Other_Agent;
	
	public Inter_agent_constraint_encap(STN_Vertex stVtx1, STN_Vertex stVtx2 , String strAgentName) 
	{
		super(stVtx1, stVtx2);
		m_str_Other_Agent = strAgentName;
	}	

	public String GetCoupledAgentName()
	{
		return m_str_Other_Agent;
	}
}
