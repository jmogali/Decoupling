package Network_Related;

public class Coupling_Constraint {

	// m_clVtx1 - m_clVtx2 + slack = m_dVal
	String m_strVtx1 , m_strVtx2;
	double m_dVal;
	
	String m_strName;
	
	public Coupling_Constraint(String strName , String strVtx1 , String strVtx2 , double dVal)
	{
		m_strVtx1 = strVtx1;
		m_strVtx2 = strVtx2;
		m_dVal = dVal;
		m_strName = strName;
	}
}
